{
  "name": "enter-exit-logger",
  "version": "1.1.0",
  "description": "typescript/javascript method enter/exit logger based on log4js-node",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "engines": {
    "node": ">=8.0"
  },
  "scripts": {
    "test": "node dist/test/disposable-test.js && node dist/test/xlog-test.js",
    "clean": "rm -rf dist log4js*.log",
    "build": "npm run clean && tsc -p . && copyfiles src/test/config/* dist/test/config",
    "tslint": "tslint ./*.ts ./lib/*.ts"
  },
  "author": {
    "name": "Walter Leinert",
    "email": "walter.leinert@outlook.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WalterLeinert/enter-exit-logger.git"
  },
  "bugs": {
    "url": "https://github.com/WalterLeinert/enter-exit-logger/issues"
  },
  "tags": [
    "logging",
    "disposable",
    "typescript",
    "log4js"
  ],
  "dependencies": {
    "log4js": "^6.3.0"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^6.0.46",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "typescript": "^4.2.3"
  }
}
